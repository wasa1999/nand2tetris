/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=a,out=w1);
    Nand(a=b,b=b,out=w2);
    Nand(a=w1,b=w2,out=w3);
    Nand(a=a,b=b,out=w4);
    Nand(a=w3,b=w4,out=w5);
    Nand(a=w5,b=w5,out=out);
    
}